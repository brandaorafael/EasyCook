<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.taqtile.easycook" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SearchActivity"
            android:label="@string/title_activity_search" >
        </activity>
        <activity
            android:name=".SearchListActivity"
            android:label="@string/title_activity_search_list" >
        </activity>
        <activity
            android:name=".SearchActivityList"
            android:label="@string/title_activity_search_activity_list" >
        </activity>
        <activity
            android:name=".PreparationActivity"
            android:label="@string/title_activity_preparation" >
        </activity>
    </application>

</manifest>
